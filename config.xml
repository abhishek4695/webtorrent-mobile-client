<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="com.streamapp.webtorrentmobile" id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WebTorrent Mobile</name>
    <description>
        Share files using the WebTorrent protocol (BitTorrent over WebRTC).
    </description>
    <author email="abhishek154@live.com" href="http://cordova.io">
        Abhishek Kumar
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="blob:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-clipboard" spec="^1.1.1" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
    <plugin name="com.markbirbeck.cordova.plugin.AppBarPlugin" spec="D:\WebProjects\cordova-plugin-android-app-bar-master" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="nl.madebymark.share" spec="~0.1.1" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
